{
  "name": "solitaire",
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "bower": "^1.8.2",
    "cross-env": "^7.0.2",
    "eslint": "^4.9.0",
    "http-server": "^0.10.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "nodemon": "^2.0.4",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "_build": "webpack app/app.js -o app/dist/app.bundle.js",
    "build": "webpack --env production",
    "_watch": "webpack --watch app/app.js -o app/dist/app.bundle.js",
    "watch": "npm run build -- --watch",
    "_prestart": "npm install",
    "_start": "http-server -c-1 -o",
    "_start_": "webpack-dev-server --open",
    "start": "nodemon -w webpack.config.js -x webpack-dev-server -- --env development",
    "postinstall": "node node_modules/bower/bin/bower install",
    "_pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "build:stats": "webpack app/app.js app/dist/app.bundle.js --json > stats.json"
  }
}
